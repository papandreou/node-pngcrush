{
  "name": "pngcrush",
  "version": "1.0.0",
  "description": "The pngcrush utility as a readable/writable stream",
  "main": "lib/PngCrush.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "gettemporaryfilepath": "=0.0.1",
    "memoizeasync": "=0.8.0",
    "pngcrush-bin": "^3.0.0",
    "which": "=1.1.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "0.4.4",
    "jshint": "2.9.3",
    "mocha": "3.0.2",
    "sinon": "1.17.5",
    "unexpected": "10.15.1",
    "unexpected-sinon": "10.3.0",
